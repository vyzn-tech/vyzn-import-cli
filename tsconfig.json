{
    "compilerOptions": {
      "module": "ES2022",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "es2017",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "incremental": true,
      "skipLibCheck": true,
      "moduleResolution":"node"
    },
    "include": ["src"],
    "exclude": ["node_modules"],
    
}